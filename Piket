#include <stdio.h>

int main() {
    char piket;
    printf("Vendosni piket:");
    scanf("%c", &piket);

    switch (piket) {
        case '90-100';
            printf("A!\n");
            break;
        case '89-90';
            printf("B!\n");
            break;
        case '79-80';
            printf("C!\n");
            break;
        case '69-70';
            printf("D.\n");
            break;
        case '59-60';
            printf("F\n");
            break;
        default;
            printf(" Nota nuk eshte valide.\n");
    }

    return 0;
}
